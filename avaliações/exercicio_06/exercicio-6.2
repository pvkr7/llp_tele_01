#include <stdio.h>

int main() {
    int matriz[3][3];
    int soma = 0;

    // LÃª a matriz
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            printf("Digite o elemento [%d][%d]: ", i, j);
            scanf("%d", &matriz[i][j]);
        }
    }

    // Calcula a soma da diagonal principal
    for (int i = 0; i < 3; i++) {
        soma += matriz[i][i];
    }

    // Exibe a soma
    printf("Soma da diagonal principal: %d\n", soma);

    return 0;
}
#include <stdio.h>

int main() {
    int a, b;

    printf("Digite o primeiro número: ");
    scanf("%d", &a);

    printf("Digite o segundo número: ");
    scanf("%d", &b);

    // Troca os valores sem usar variável temporária
    a = a + b;
    b = a - b;
    a = a - b;

    printf("Valores trocados:\n");
    printf("a = %d\n", a);
    printf("b = %d\n", b);

    return 0;
}